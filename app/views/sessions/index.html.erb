<p id="notice"><%= notice %></p>

<h1>Sessions</h1>

<table>
  <thead>
    <tr>
      <th>Patient</th>
      <th>User</th>
      <th>Therapy</th>
      <th>Treatment</th>
      <th>Date</th>
      <th>Time</th>
      <th>Symptom</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sessions.each do |session| %>
      <tr>
        <td><%= session.patient_id %></td>
        <td><%= session.user_id %></td>
        <td><%= session.therapy %></td>
        <td><%= session.treatment %></td>
        <td><%= session.date %></td>
        <td><%= session.time %></td>
        <td><%= session.symptom %></td>
        <td><%= link_to 'Show', session %></td>
        <td><%= link_to 'Edit', edit_session_path(session) %></td>
        <td><%= link_to 'Destroy', session, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Session', new_session_path %>
